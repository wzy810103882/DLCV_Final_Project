# vanilla train
python main_train.py --input_name background_001_500.png

# change min max image size and train
python main_train.py --input_name background_001_500.png --min_size 50 --max_size 500
python main_train.py --input_name orange.png --min_size 5 --max_size 50

# paint to image (tend to style transfer)
python paint2image.py --input_name orange.png --ref_name baseball.png --paint_start_scale 0

# style transfer
python style_transfer.py --input_name orange.png --st_input_name baseball.png --mode style_transfer --st_start_scale 1
-
# random samples (same size and arbitrary size)
python random_samples.py --input_name background_001_500.png --mode random_samples_arbitrary_sizes --gen_start_scale 1
python random_samples.py --input_namme orange.png --mode random_samples_arbitrary_sizes --gen_start_scale 1
python random_samples.py --input_name orange.png --mode random_samples --gen_start_scale 1

# harmoinzation
python harmonization.py --input_name background_001_500.png --ref_name human.png --harmonization_start_scale 1

# generate collage
python image_collage.py --train_img Input/Images/orange.png --test_img Input/StyleTransfer/handbag.png --result_dir /Users/ouchouyang/Desktop/StyleTransfer/handbag/handbag_out

# generate masks by MaskRCNN
python maskrcnn.py --img_path Input/Harmonization/plane.png --threshold 0.9



